  "scripts": {
    "start": "concurrently 'npm run start:client' 'npm run start:server'",
    "start:client": "react-scripts start",
    "start:server": "NODE_ENV=development nodemon --ignore '.*.swp' --watch server server/index.js --exec babel-node --presets react-app",
    "serve:server": "node dist/index.js",
    "build": "npm run build:client && npm run build:server",
    "build:client": "react-scripts build",
    "build:server": "NODE_ENV=production babel server -d dist --presets react-app",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "redux:add": "node custom-scripts/redux.js"
  }
}
